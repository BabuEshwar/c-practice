#include<stdio.h>
int main(){
	int i, n1, n2, count = 0, sum = 0;
	printf("Enter the numbers range n1 and n2");
	scanf("%d %d", n1, n2);
	for(i = n1; i <= n2; i++){
		if(i % 5 == 0){
			printf("%d", i);
			count++;
			sum = sum + i;
		}
	}
	printf("The numbers divisible by 5 between %d and %d = %d\n", n1, n2, count);
	printf("The sum of numbers divisibl by 5 = %d\n", sum);
	return 0;
}
